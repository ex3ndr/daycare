{
  "name": "daycare-cli",
  "version": "0.0.1",
  "description": "Local-first, plugin-driven multi-agent runtime",
  "type": "module",
  "license": "MIT",
  "author": "ex3ndr",
  "repository": {
    "type": "git",
    "url": "https://github.com/ex3ndr/daycare.git",
    "directory": "packages/daycare"
  },
  "homepage": "https://github.com/ex3ndr/daycare",
  "bugs": {
    "url": "https://github.com/ex3ndr/daycare/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "multi-agent",
    "runtime",
    "cli"
  ],
  "bin": {
    "daycare": "./bin/daycare.js"
  },
  "files": [
    "dist",
    "bin",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && cp -r sources/prompts dist/ && cp -r sources/skills dist/skills",

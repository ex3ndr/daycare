    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@anthropic-ai/sandbox-runtime": "^0.0.32",
